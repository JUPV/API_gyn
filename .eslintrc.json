{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "camelcase": "off",
    "no-useless-constructor": "off",
    "semi": "off",
    "quotes": [
      "error",
      "single",
      { "avoidEscape": true }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}
